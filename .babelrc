{
  "presets": [
    "babel-preset-expo"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx",
          ".json"
        ],
        "alias": {
          "~": "./src/"
        },
      },
    ],
  ],
}