{
  "name": "imhere",
  "version": "1.0.0",
  "description": "",
  "main": "node_modules/expo/AppEntry.js",
  "author": "LeoMSSilva",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --clear --android",
    "ios": "expo start --clear --ios",
    "web": "expo start --clear --web",
    "clean": "rm -rf .expo node_modules yarn.lock && yarn",
    "format": "prettier -w .",
    "lint": "eslint . --fix",
    "commit": "cz",
    "configure-husky": "husky install && husky add .husky/pre-commit 'yarn lint-staged' && husky add .husky/prepare-commit-msg 'exec < /dev/tty && yarn cz --hook || true' && husky add .husky/commit-msg 'yarn commitlint -e $1' && husky add .husky/pre-push'expo publish'",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "expo": "~47.0.8",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-web": "~0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/eslint-plugin": "^1.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "typescript": "^4.6.3"
  }
}
